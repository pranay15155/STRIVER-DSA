https://www.geeksforgeeks.org/problems/bfs-traversal-of-graph/1


class Solution {
  public:
    // Function to return Breadth First Traversal of given graph.
    vector<int> bfsOfGraph(int V, vector<int> adj[]) {
        vector<int>ans;
        queue<int>q;
        q.push(0);
        vector<int>vis(V,0);
        vis[0]=1;
        while(q.size()){
            int s=q.size();
            for(int i=0;i<s;i++){
                int node=q.front();
                q.pop();
                ans.push_back(node);
                for(auto &v:adj[node]){
                    if(vis[v]==0){
                        vis[v]=1;
                        q.push(v);
                    }
                }
            }
        }
        return ans;
    }
};
